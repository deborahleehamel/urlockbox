<div class="container">

  <h1> Log in</h1>
  <%= form_for(:session, url: login_path) do |f| %>
    <%= f.label :email, "Email" %>
    <%= f.email_field :email %><br>
    <%= f.label :password %>
    <%= f.password_field :password %><br><br>

    <%= f.submit "Log in", :class => "btn btn-success" %>
  <% end %>

  Or, if you are brand new to URLockbox:<br>
  <%= link_to("Sign Up", new_user_path) %>
</div>
